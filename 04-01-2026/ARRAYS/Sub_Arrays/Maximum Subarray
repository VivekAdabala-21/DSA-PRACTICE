class Solution {
    public int maxSubArray(int[] nums) {
        int n=nums.length;
        int maxSum= Integer.MIN_VALUE;

        for(int st=0;st<n;st++)
        {
            int currSum=0;
            for(int end=st;end<n;end++)
            {
              currSum += nums[end];
               maxSum=Math.max(maxSum,currSum);
            }
            
        }
        return maxSum;

        
    }
}
