class Solution {
    public int removeElement(int[] nums, int val) {
        
        int n=nums.length;
        int a=0;
        int k=-1;
        int count=0;
        
        for(int i=0;i<n;i++)
        {
          if(nums[i]!=val)
          {
            count++;
              nums[a]=nums[i];
              a++;
          }
        }
        return count;
    }
}
