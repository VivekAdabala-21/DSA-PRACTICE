class Solution {
    List<Integer> list = new ArrayList<>();

    public List<Integer> preorderTraversal(TreeNode root) {
        preorder(root);
        return list;
    }

    void preorder(TreeNode a) {
        if (a == null) {
            return;
        }
        list.add(a.val);
        preorder(a.left);
        preorder(a.right);
    }

}
