class Solution {
    List<Integer>list = new ArrayList<>();
    public List<Integer> inorderTraversal(TreeNode root) {
        inorder(root);
        return list;
    }
    void inorder(TreeNode a)
    {
        if(a==null)
        {
            return;
        }
        inorder(a.left);
        list.add(a.val);
        inorder(a.right);
    }
}
