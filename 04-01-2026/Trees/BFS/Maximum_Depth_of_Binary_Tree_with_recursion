class Solution {
    
    public int maxDepth(TreeNode root) {
       
        
      return  pro(root);
    }
    public int pro(TreeNode a)
    {
    
        if(a==null) return 0;
    
        int left=pro(a.left);
        int right=pro(a.right);
        return 1+Math.max(left,right);
    } 
    
}
